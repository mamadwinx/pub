  <script>
    const highlightsToggle = document.querySelector("[data-highlights-target='highlightsToggle']");
    const highlights = document.querySelector("[data-highlights-target='highlights']");

    if (highlightsToggle && highlights) {
      let highlightsEnabled = localStorage.getItem("coinsIndex:highlights");
      highlightsEnabled = highlightsEnabled === null ? highlightsToggle.checked : highlightsEnabled === "true";

      if (highlightsEnabled !== highlightsToggle.checked) {
        highlightsToggle.checked = highlightsEnabled;
        highlights.classList.toggle("tw-hidden", !highlightsEnabled);
        highlights.classList.toggle("!tw-hidden", !highlightsEnabled);
      }
    }
  </script>

  <!-- Chart section -->
  
